.container{
    padding: 0;
    margin: 0;
    width: 100%;
    height: 100vh;
}
.logo image{
    width: 750rpx;
    height: 456rpx;
}
.loginBox{
    width: 750rpx;
}
.login{
    margin: 0 0 30rpx 30rpx;
    width: 686rpx;
    height: 90rpx;
    border: 1rpx solid #abe6bf;
    border-radius: 80rpx;
    text-align: center;
    color: #59c465;
    line-height: 90rpx;
}
.login image{
    vertical-align:middle;
    padding-right: 10rpx;
    width: 42rpx;
    height: 42rpx;
}
.notice{
    padding: 69.5rpx 30rpx 0 30rpx;
}
.specialNotice{
    line-height: 35rpx;
    font-size: 25.35rpx;
    color: #999999;
}
.outline{
    font-weight: bold;
    color: #000000;
}
.webNotice{
    line-height: 35rpx;
    font-size: 25.35rpx;
    color: #999999;
}